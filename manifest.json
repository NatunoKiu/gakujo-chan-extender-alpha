{
    "manifest_version":2,
    "name":"gakujo-chan-Extender",
    "version":"0.1",

    "description":"gakujo-chan-Extender",

    "icons":{
        "48":"icons/icon48.png",
        "128":"icons/icon128.png"
    },

    "content_scripts":[
        {
            "matches":["https://gakujo.iess.niigata-u.ac.jp/campusweb/campusportal.do?page=main*"],
            "all_frames": true,
            "js":["autoExtender.js"]
        },
        {
            "matches":["https://gakujo.iess.niigata-u.ac.jp/campusweb/campusportal.do?page=main&tabId=en"],
            "all_frames": true,
            "run_at": "document_end",
            "js":["reportSorter.js"]
        },
        {
            "matches":["https://gakujo.iess.niigata-u.ac.jp/campusweb/campusportal.do?page=main&tabId=si"],
            "all_frames": true,
            "run_at": "document_end",
            "js":["gpaSolver.js"]
        }
    ]
}